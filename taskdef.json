{
    "family": "hello-world",
    "requiresCompatibilities": [
	    "FARGATE"
    ],
    "networkMode": "awsvpc",
    "cpu": "256",
    "memory": "512",
    "containerDefinitions": [
        {
            "image": "%REPOSITORY_URI%:v_%BUILD_NUMBER%",
            "name": "hello-world",
            "cpu": 10,
            "memory": 256,
            "essential": true,
            "portMappings": [
                {
                    "containerPort": 80,
                    "hostPort": 80
                }
            ]
        }
    ]
}
